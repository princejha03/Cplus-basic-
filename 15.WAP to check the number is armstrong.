#include<iostream>
using namespace std;
int main(){
    int num,temp,sum=0;
    cout << "Enter the number: ";
    cin >> num;
    temp = num;
    while(num > 0){
        int rem = num%10;
        sum = sum + (rem*rem*rem);
        num = num/10;
    }
    if(temp == sum){
        cout <<"The is armstrong:";
    }
    else{
        cout << "The is not armstrong";
    }
    return 0;
}
