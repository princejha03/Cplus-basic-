#include <iostream>
using namespace std;
int factorial(int n) {
    int result = 1, i;   //"sum =1"//
    for (i = 2; i <= n; i++) {
        result *= i;
    }
    return result;
}

int main() {
    int num;
    cout << "Enter the number: ";
    cin >> num;
    cout << "The factorial of " << num << " is: " << factorial(num);
    return 0;
}
