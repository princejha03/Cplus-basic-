#include<iostream>
using namespace std;
int reverse(int num){
    int rev =0;
    while(num>0){
        rev = rev*10 + num%10;
        num = num/10;
    }
    return rev;
}
int main(){
    int num;
    cout << "enter the number: ";
    cin >> num;
    cout << "before reverse\n" << num <<"\n" <<"after reverse\n" << reverse(num);
    return 0;
}
