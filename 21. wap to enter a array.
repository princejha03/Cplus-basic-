#include<iostream>
using namespace std;
int main(){
    int n,i;
    cout << "Enter the number: ";
    cin >> n;
    int arr[n];
    cout << "enter " << n << " in the array:\n";
    for(i=0;i<n;i++){
        cin >> arr[i];
    }
    cout << "The array is: \n";
    for(i=0;i<n;i++){
        cout << arr[i] << " ";
    }
    return 0;
}
